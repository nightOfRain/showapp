    </feature>
    <feature name="NetworkStatus" >
        <param
            name="android-package"
            value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>

    <feature name="NTNewHttpCommunication" >
        <param
            name="android-package"
            value="com.nantian.element.http.plugin.NTHttpPlugin" />
    </feature>
    
    <feature name="NTPictureTaker" >
        <param
            name="android-package"
            value="com.nantian.element.camera.plugin.NTCameraPlugin" />
