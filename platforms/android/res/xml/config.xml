    <feature name="CallPhone" >
        <param
            name="android-package"
            value="com.nantian.plugins.CallPhone" />
    </feature>
        <feature name="ImagePicker">
        <param
            name="android-package"
            value="com.nantian.plugins.ImagePicker" />
    </feature>
        <feature name="BaiduLocation" >
        <param
            name="android-package"
            value="com.nantian.plugins.BaiduLocation" />
