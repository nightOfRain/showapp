            name="android-package"
            value="com.nantian.element.camera.plugin.NTCameraPlugin" />
    </feature>

    <feature name="Ntfile" >
        <param
            name="android-package"
            value="com.nantian.element.fileupload.plugin.NTFilePlugin" />
    </feature>
    
    <feature name="NTDataDictionary" >
        <param
            name="android-package"
            value="com.nantian.plugins.NTDataDictionaryPlugin" />
    </feature>
    
    <feature name="NTUICtrl" >
        <param
            name="android-package"
            value="com.nantian.plugins.NTUIPlugin" />
    </feature>
    
    <feature name="NTKeyboard" >
        <param
            name="android-package"
            value="com.nantian.element.keyboard.plugin.NTKeyBoardPlugin" />
    </feature>
    
    <feature name="NTSecret" >
        <param
            name="android-package"
